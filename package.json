{
  "name": "vuensee",
  "version": "0.9.12",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint '*.js' 'src/**/*.js' 'src/**/*.vue'"
  },
  "dependencies": {
    "@novnc/novnc": "^1.2.0",
    "vue": "^3.0.5",
    "vue-i18n": "^9.0.0-rc.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "sass": "^1.32.6",
    "vite": "^2.0.0-beta.62"
  },
  "optionalDependencies": {
    "eslint": "^7.15.0",
    "eslint-plugin-vue": "^7.2.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended"
    ],
    "globals": {
      "process": true
    },
    "rules": {
      "no-unused-vars": 1,
      "vue/component-tags-order": [
        "error",
        {
          "order": [
            "template",
            "style",
            "script"
          ]
        }
      ]
    },
    "ignorePatterns": [
      "*.test.js"
    ]
  }
}
